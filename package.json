{
  "name": "zeroui-m5",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^5.3.3",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.0.0"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register --ui tdd \"src/**/*.test.ts\""
  },
  "contributes": {
    "commands": [
      {
        "command": "zeroui.m5.openPanel",
        "title": "Open M5 Panel"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "zeroUI.container",
          "title": "ZeroUI",
          "icon": "$(beaker)"
        }
      ]
    },
    "views": {
      "zeroUI.container": [
        {
          "id": "zeroUI.view.main",
          "name": "Main"
        }
      ]
    }
  }
}

